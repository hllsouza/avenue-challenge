{
  "name": "openweather-cypress-api",
  "version": "1.0.0",
  "description": "Projeto de Testes de API do OpenWeather",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run --project ./api",
    "e2e:api": "npx cypress run --project ./api --record --tag 'api-weather' --spec api/cypress/e2e/*.cy.js",
    "e2e:amazon": "npx cypress run --project ./web --record --key --spec web/cypress/e2e/amazon.cy.js",
    "e2e:avenue": "npx cypress run --project ./web --record --key --spec web/cypress/e2e/avenue.cy.js"
  },
  "keywords": [
    "cypress",
    "api",
    "openweather"
  ],
  "author": "Harllen S",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^12.17.3",
    "cypress-plugin-api": "^2.11.1",
    "cypress-tesults-reporter": "^1.4.1",
    "dotenv": "^16.3.1",
    "tesults": "^1.1.3"
  }
}
